{
  "name": "graasp-library",
  "version": "1.16.0",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "next dev -p 3005",
    "start:test": "NODE_ENV='test' env-cmd -f ./.env.test next dev -p 3005",
    "start:ci": "yarn build && yarn start:next",
    "start:next": "next start",
    "start:prod": "NODE_ENV=production node build/server.js",
    "build": "next build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "test:once": "exit 0",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{app,src,pages,cypress}/**/*.{js,ts,tsx}'",
    "prettier:write": "prettier --write '{app,src,pages,cypress}/**/*.{js,ts,tsx}'",
    "check": "yarn lint && yarn prettier:check && yarn tsc --noEmit",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "release:patch": "standard-version -a --release-as patch",
    "postinstall": "husky install",
    "post-commit": "git status",
    "pre-commit": "yarn check",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress": "concurrently -k -s first \"yarn start:test\" \"wait-on http://localhost:3005 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome --spec \"cypress/**/*.(spec|cy).js\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "client:generate": "openapi-ts"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.14.0",
    "@graasp/query-client": "5.8.0",
    "@graasp/sdk": "5.11.0",
    "@graasp/stylis-plugin-rtl": "2.2.0",
    "@graasp/translations": "1.43.0",
    "@graasp/ui": "5.5.1",
    "@hey-api/client-fetch": "0.7.2",
    "@mui/icons-material": "6.4.1",
    "@mui/lab": "6.0.0-beta.24",
    "@mui/material": "6.4.1",
    "@sentry/nextjs": "8.51.0",
    "@tanstack/react-query": "5.65.1",
    "date-fns": "4.1.0",
    "http-status-codes": "2.3.0",
    "i18next": "23.16.8",
    "interweave": "13.1.0",
    "katex": "0.16.21",
    "lodash.truncate": "4.4.2",
    "lucide-react": "0.453.0",
    "next": "14.2.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-ga4": "2.1.0",
    "react-helmet": "6.1.0",
    "react-i18next": "15.4.0",
    "react-quill": "2.0.0",
    "react-router-dom": "6.28.2",
    "react-toastify": "10.0.6",
    "social-links": "1.14.0",
    "stylis": "4.3.5",
    "uuid": "11.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@cypress/code-coverage": "3.13.10",
    "@hey-api/openapi-ts": "0.63.0",
    "@storybook/addon-a11y": "8.5.2",
    "@storybook/addon-docs": "8.5.2",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/addon-interactions": "8.5.2",
    "@storybook/addon-links": "8.5.2",
    "@storybook/blocks": "^8.5.2",
    "@storybook/nextjs": "^8.5.2",
    "@storybook/react": "^8.5.2",
    "@storybook/test": "^8.5.2",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/katex": "^0.16.7",
    "@types/lodash.truncate": "4.4.9",
    "@types/node": "20.17.16",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-helmet": "6.1.11",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@typescript-eslint/parser": "8.22.0",
    "concurrently": "9.1.2",
    "cypress": "13.17.0",
    "env-cmd": "10.1.0",
    "eslint": "9.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-app": "6.2.2",
    "eslint-plugin-storybook": "0.11.2",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "nyc": "17.1.0",
    "prettier": "3.4.2",
    "standard-version": "9.5.0",
    "storybook": "^8.5.2",
    "typescript": "5.7.3",
    "wait-on": "8.0.2"
  },
  "nyc": {
    "exclude": [
      "pages/api/__coverage__.js"
    ]
  },
  "packageManager": "yarn@4.5.3",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
