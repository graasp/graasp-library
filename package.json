{
  "name": "graasp-library",
  "version": "2.1.0",
  "license": "AGPL-3.0-only",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3002",
    "start": "pnpm dev",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "start:test": "vite build --mode test && vite preview",
    "preview": "node .output/server/index.mjs",
    "prettier:write": "prettier --write '{src,tests}/**/*.{js,ts,tsx}'",
    "check:prettier": "prettier --check '{src,tests}/**/*.{js,ts,tsx}'",
    "check:lint": "eslint '{src,tests}/**/*.{js,ts,tsx}'",
    "check:type": "tsc --noEmit",
    "check": "pnpm run \"/^check:.*/\"",
    "post-commit": "git status",
    "pre-commit": "pnpm check",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "vitest --project unit",
    "test:component": "vitest --project storybook",
    "test:e2e": "playwright test",
    "test": "pnpm run \"/^test:.*/\"",
    "client:generate": "openapi-ts",
    "machine-translate": "inlang machine translate --project project.inlang",
    "mockserver": "tsx tests/mockServer/index.ts",
    "mockserver:watch": "tsx watch tests/mockServer/index.ts"
  },
