  "type": "module",
  "scripts": {
    "dev": "vinxi dev --port 3002",
    "start": "vinxi start",
    "build": "vinxi build",
    "start:test": "vinxi build --mode test && vinxi start --mode test",
    "preview": "node .output/server/index.mjs",
    "prettier:write": "prettier --write '{src,cypress}/**/*.{js,ts,tsx}'",
    "check:prettier": "prettier --check '{src,cypress}/**/*.{js,ts,tsx}'",
    "check:lint": "eslint .",
    "check:type": "tsc --noEmit",
    "check": "pnpm run \"/^check:.*/\"",
    "postinstall": "husky",
    "post-commit": "git status",
    "pre-commit": "pnpm check",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "vitest --project unit",
    "test:component": "vitest --project storybook",
    "test:e2e": "playwright test",
    "test": "pnpm run \"/^test:.*/\"",
    "client:generate": "openapi-ts",
    "machine-translate": "inlang machine translate --project project.inlang"
  },
